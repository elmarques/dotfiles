layout {
  default_tab_template {
    children
    pane size=1 borderless=true {
      plugin location="zellij:compact-bar"
    }
  }
  tab name="editor" focus=true {
    pane command="nvim"
  }
  tab name="terminal" {
    pane
  }
  tab name="git" {
    pane command="lazygit" start_suspended=true
  }
}
