[alias]
	br = branch
	cm = commit -m
	co = checkout
	df = diff --color --color-words --abbrev
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	st = status -s
[core]
	editor = $EDITOR
[color]
	ui = auto
[color "branch"]
	current = yellow
	local = cyan bold
	remote = green bold
[color "diff"]
	frag = magenta bold
	meta = yellow bold
	new = green bold
	old = red bold
[color "status"]
	added = yellow bold
	changed = green bold
	untracked = cyan bold
[pull]
	rebase = true
[merge]
	conflictstyle = diff3
[diff]
	colorMoved = default
[user]
	name = El Marques
	email = el@dachs.tech
